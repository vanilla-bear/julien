{% if view.current_display == 'block_portfolio_masonry_style' %}
	{% if rows %}
	<div class="relative">
		{% set nodes = rows.0.0['#rows'] %}
		{% set n = 0 %}
		<!-- ITEMS GRID -->
        <ul class="port-grid clearfix masonry" id="items-grid">
		{% for item in nodes %}
			{% set n = n + 1 %}
			{% set node = item['#row'].nid|str_replace(' ', '')|node_load %}
			<li class="port-item mix">
				<a href="{{ path('entity.node.canonical', {'node': node.id}) }}">
					<div class="port-img-overlay">
					{% if n == 1 or n == 6 or n == 7 or n == 8 or n == 13 or n ==  14 %}
						<img class="port-main-img" src="{{ node.field_images.0.entity.fileuri|image_style_url('image_650x415') }}" alt="{{ node.field_images.0.alt }}" >
					{% elseif n == 3 or n == 10 %}
						<img class="port-main-img" src="{{ node.field_images.0.entity.fileuri|image_style_url('image_650x1245') }}" alt="{{ node.field_images.0.alt }}" >
					{% else %}
						<img class="port-main-img" src="{{ node.field_images.0.entity.fileuri|image_style_url('image_650x830') }}" alt="{{ node.field_images.0.alt }}" >
					{% endif %}
					</div>
				</a>
				<div class="port-overlay-cont">
					<div class="port-title-cont">
						<h3><a href="{{ path('entity.node.canonical', {'node': node.id}) }}">{{ node.title.value }}</a></h3>
						<span>{{ node.field_portfolio_categories.value }}</span>
					</div>
					<div class="port-btn-cont">
						<a href="{{ file_url(node.field_images.0.entity.fileuri) }}" class="lightbox mr-20" >
							<div aria-hidden="true" class="icon_search"></div>
						</a>
						<a href="{{ path('entity.node.canonical', {'node': node.id}) }}">
							<div aria-hidden="true" class="icon_link"></div>							
						</a>
					</div>
				</div>
			</li>
		{% endfor %}
        </ul>
	</div>
	{% endif %}
	{{ footer }}

{% elseif view.current_display == 'page_portfolio' %}
	{% set portfolio_style_temp = 'style'|get_path_param %}
	{% if portfolio_style_temp %}
		{% set portfolio_style = portfolio_style_temp %}
	{% endif %}
	{% if portfolio_style is empty %}
		{% set portfolio_style = 'grid' %}
	{% endif %}
	{% set terms = get_list_term('portfolio_categories') %}
	{% if portfolio_style == 'grid' %}
		<div class="p-140-cont">
		{% for term in terms %}
			<div class="container">
            	<div class="row mb-80">
            		<!-- SIDEBAR -->
	              	<div class="col-sm-4 col-md-3">
		                <h2 class="section-title-3 mb-30 uppercase">{{ term.name }}</h2>
		                <div class="mb-50">
		                  	<p>{{ term.description__value }}</p>
		                </div>
		            </div>
		            <div class="col-sm-8 col-md-offset-1 ">
                		<div class="row">
                			{% set tid = term.tid %}
							{{ drupal_view('_hawell_portfolio', 'embed_portfolio_in_term', tid) }}
              			</div>
              		</div>
				</div>
			</div>
			{% if loop.last == false %}
			<!-- DIVIDER -->
          	<hr class="mt-0 mb-80">
          	{% endif %}
		{% endfor %}
		</div>
